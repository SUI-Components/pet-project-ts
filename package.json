{
  "name": "@mv/web-server",
  "version": "1.0.0",
  "private": true,
  "description": "Pet project used as a explanation for the landing project of the newcomers.",
  "main": "index.js",
  "workspaces": [
    "components/**",
    "domain",
    "theme"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.mpi-internal.com:scmspain/frontend-all--pet-project-skeleton.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "co": "sui-mono commit",
    "test": "npm run test --workspace=domain",
    "dev": "sui-bundler dev",
    "ssr": "sui-bundler build -C && sui-ssr build -C && node server/index.js",
    "phoenix": "npx @s-ui/mono@2 phoenix && npx -y ultra-runner --raw --recursive prepublishOnly",
    "studio:dev": "sui-studio dev",
    "studio:generate": "sui-studio generate -P sui -S adv-ui --swc",
    "lint": "sui-lint js && sui-lint sass",
    "pre-commit": "npm run lint",
    "pre-push": "npm run test"
  },
  "dependencies": {
    "@adv-ui/sui-layout-header": "1",
    "@mv/domain": "1",
    "@s-ui/component-peer-dependencies": "1",
    "@s-ui/hoc": "typescript",
    "@s-ui/i18n": "1.24.0",
    "@s-ui/js-compiler": "typescript",
    "@s-ui/react-context": "typescript",
    "@s-ui/react-head": "1.7.0",
    "@s-ui/react-initial-props": "2.15.0",
    "@s-ui/react-router": "1.9.0",
    "@s-ui/studio": "typescript",
    "@s-ui/studio-utils": "1.10.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.11",
    "eslint-config-standard-react": "13.0.0",
    "tslint-no-unused-expression-chai": "0.1.4"
  },
  "overrides": {
    "@s-ui/bundler": "typescript",
    "@s-ui/helpers": "typescript",
    "@s-ui/hoc": "typescript",
    "@s-ui/js-compiler": "typescript",
    "@s-ui/react-context": "typescript",
    "@s-ui/studio": "typescript",
    "@s-ui/lint": "typescript",
    "@s-ui/test": "typescript"
  },
  "devDependencies": {
    "@s-ui/bundler": "typescript",
    "@s-ui/lint": "typescript",
    "@s-ui/mono": "2.27.0",
    "@s-ui/ssr": "8.3.0",
    "@types/chai": "4.3.3",
    "@types/chai-dom": "0.0.13",
    "@types/mocha": "10.0.0",
    "typescript": "5.0.2"
  },
  "config": {
    "sui-bundler": {
      "type": "typescript"
    }
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js"
    ]
  },
  "stylelint": {
    "extends": "./node_modules/@s-ui/lint/stylelint.config.js"
  },
  "prettier": "./node_modules/@s-ui/lint/.prettierrc.js"
}