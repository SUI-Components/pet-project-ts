{
  "name": "@mv/web-server",
  "version": "1.0.0",
  "private": true,
  "description": "Pet project used as a explanation for the landing project of the newcomers.",
  "main": "index.js",
  "workspaces": [
    "components/**",
    "domain",
    "theme"
  ],
  "scripts": {
    "co": "sui-mono commit",
    "test": "exit 0",
    "dev": "sui-bundler dev",
    "ssr": "sui-bundler build -C && sui-ssr build -C && node server/index.js --inspect `pbpaste`",
    "phoenix": "npx @s-ui/mono@2 run 'rm -rf ./node_modules' && rm -rf ./node_modules && npm install --legacy-peer-deps",
    "studio:dev": "sui-studio dev",
    "studio:generate": "sui-studio generate -P sui -S adv-ui --swc",
    "lint": "sui-lint js --staged && sui-lint sass --staged",
    "pre-commit": "npm run lint",
    "pre-push": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.mpi-internal.com:scmspain/frontend-all--pet-project-skeleton.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "config": {
    "sui-bundler": {
      "parser": {
        "syntax": "typescript"
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js"
    ]
  },
  "stylelint": {
    "extends": "./node_modules/@s-ui/lint/stylelint.config.js"
  },
  "prettier": "./node_modules/@s-ui/lint/.prettierrc.js",
  "devDependencies": {
    "@s-ui/lint": "4.29.0-typescript.0",
    "@s-ui/mono": "2.27.0",
    "@s-ui/ssr": "8.3.0",
    "@types/chai": "4.3.3",
    "@types/chai-dom": "0.0.13",
    "@types/mocha": "10.0.0",
    "typescript": "4.3.2"
  },
  "dependencies": {
    "@adv-ui/sui-layout-header": "1",
    "@mv/domain": "1",
    "@s-ui/bundler": "9.37.0-typescript.1",
    "@s-ui/component-peer-dependencies": "1.9.0",
    "@s-ui/hoc": "1.34.0",
    "@s-ui/i18n": "1.24.0",
    "@s-ui/js-compiler": "1.20.0-typescript.0",
    "@s-ui/react-context": "1.7.0",
    "@s-ui/react-head": "1.7.0",
    "@s-ui/react-initial-props": "2.15.0",
    "@s-ui/react-router": "1.9.0",
    "@s-ui/studio": "11.22.0-typescript.0",
    "@s-ui/studio-utils": "1.10.0",
    "@types/react": "18.0.21",
    "eslint-config-standard-react": "^13.0.0",
    "tslint-no-unused-expression-chai": "0.1.4"
  }
}